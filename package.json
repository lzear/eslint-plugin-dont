{
  "type": "module",
  "name": "eslint-plugin-dont",
  "description": "Optimized ESLint Rules for Elite Coders",
  "version": "2.2.0",
  "homepage": "https://www.dont.ninja",
  "repository": "https://github.com/lzear/eslint-plugin-dont",
  "author": "lzear",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "dont",
    "10x",
    "10x engineer"
  ],
  "scripts": {
    "build": "vite build",
    "docs:dev": "vitepress dev",
    "docs:generate": "eslint-doc-generator --rule-list-columns name,description,fixable,hasSuggestions,deprecated --path-rule-list readme.md --url-rule-doc https://www.dont.ninja/rules/{name} --url-configs https://www.dont.ninja/configs && eslint-doc-generator --rule-list-columns name,description,fixable,hasSuggestions,deprecated --rule-doc-title-format name --path-rule-list ./docs/rules/index.md --url-rule-doc /rules/{name} --url-configs /configs/ && prettier --write readme.md ./docs/rules/index.md",
    "docs:build": "vitepress build",
    "release": "pnpm release:check && pnpm release:version && pnpm release:publish",
    "release:check": "pnpm test && pnpm run build",
    "release:publish": "clean-publish",
    "release:version": "changelogen --output changelog.md --release --push",
    "test:format": "prettier --check \"**/*.{js,ts,json,md,yml}\"",
    "test:css": "stylelint \\\".vitepress/**/*.{css,vue}\\\"",
    "test:js": "eslint \"**/*.{js,ts}\"",
    "test:types": "tsc --noEmit --pretty",
    "test:unit": "vitest --run --coverage --single-thread --no-threads",
    "test": "pnpm run /^test:/"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.mjs",
    "./configs/recommended": "./dist/configs/recommended.mjs",
    "./configs/all": "./dist/configs/all.mjs",
    "./package.json": "./package.json"
  },
  "packageManager": "yarn@1.22.19",
  "peerDependenciesMeta": {
    "astro-eslint-parser": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "peerDependencies": {
    "astro-eslint-parser": "^0.16.0",
    "eslint": ">=8.0.0",
    "svelte": ">=3.0.0",
    "svelte-eslint-parser": "^0.33.0",
    "vue-eslint-parser": ">=9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.9.1",
    "date-fns": "^2.30.0",
    "has-emoji": "^2.0.0",
    "minimatch": "^9.0.3",
    "natural-compare-lite": "^1.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@azat-io/stylelint-config": "^0.1.0",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@fontsource/comic-mono": "^5.0.7",
    "@fontsource/jetbrains-mono": "^5.0.15",
    "@types/babel__helper-validator-identifier": "^7.15.0",
    "@types/eslint": "^8.44.4",
    "@types/lodash": "^4.14.199",
    "@types/natural-compare-lite": "^1.4.1",
    "@types/node": "^20.8.10",
    "@types/prettier-linter-helpers": "^1.0.2",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@typescript-eslint/rule-tester": "^6.9.1",
    "@typescript-eslint/types": "^6.9.1",
    "@vercel/node": "^3.0.7",
    "@vitest/coverage-v8": "^0.34.6",
    "astro-eslint-parser": "^0.16.0",
    "changelogen": "^0.5.5",
    "clean-publish": "^4.2.0",
    "codemirror": "^6.0.1",
    "daisyui": "^3.9.2",
    "eslint": "^8.52.0",
    "eslint-doc-generator": "^1.5.3",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vitest": "^0.3.8",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.3",
    "prettier-linter-helpers": "^1.0.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-gamut": "^1.3.3",
    "stylelint-order": "^6.0.3",
    "stylelint-plugin-logical-css": "^0.13.2",
    "svelte": "^4.2.2",
    "svelte-eslint-parser": "^0.33.1",
    "tailwindcss": "^3.3.3",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "vitepress": "1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue": "^3.3.7",
    "vue-codemirror": "^6.1.1"
  }
}

